generator client {
  provider = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["public", "auth"]
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  created_at  DateTime  @default(now())

  @@schema("public")
}